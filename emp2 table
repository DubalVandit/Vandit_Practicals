create table emp2(ID number(2),name varchar(20),salary number(8));
                 
insert into emp2 values(1,'Dubal Vandit',50000);
insert into emp2 values(2,'Patel Yash',50000);
insert into emp2 values(3,'Shah manav',25000);

select * from emp2 where name like 'D%' or name like 'p%' or name like 's%';
